    <!--********************************* REGLES DE SV ***************************************************** -->

    <rule comment="RULE: VERB">
      <pattern>
        <pattern-item n="verbcj"/>
      </pattern>
      <action>
        <call-macro n="conj_verb1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <choose>
          <!-- PRS, PAST -->
          <when>
            <test>
              <or>
                <equal>
                  <clip pos="1" side="tl" part="a_temps"/>
                  <lit-tag v="pres"/>
                </equal>
                <equal>
                  <var n="temps_chunk"/>
                  <lit-tag v="past"/>
                </equal>
              </or>
            </test>
            <call-macro n="conj_verb1">
              <with-param pos="1"/>
            </call-macro>
            <out>
              <chunk name="lemma_verb_pres" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_verb"/></tag>
          	      <tag><var n="temps_chunk"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_pers"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                </tags>
                <lu>
                  <get-case-from pos="1">
                    <lit v="prpers"/>
                  </get-case-from>
                  <lit-tag v="prn"/>
                  <lit-tag v="subj"/>
                  <clip pos="1" side="tl" part="a_pers" />
                  <var n="genre"/>
                  <clip pos="1" side="tl" part="a_nbr" />
                </lu>
                <b/>
                <lu>
          	      <var n="verbfinal"/>
          	      <clip pos="1" side="tl" part="lemq"/>
          	    </lu>
              </chunk>
            </out>
          </when>

          <!-- FUTURE -->
          <when>
            <test>
              <equal>
                <clip pos="1" side="sl" part="a_temps"/>
                <lit-tag v="fut"/>
              </equal>
            </test>
            <call-macro n="conj_verb1">
              <with-param pos="1"/>
            </call-macro>
            <out>
              <chunk name="lemma_verb_fut" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="tl" part="a_verb"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_temps"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_pers"/></tag>
          	      <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                </tags>
                <lu>
                  <get-case-from pos="1">
                    <lit v="prpers"/>
                  </get-case-from>
                  <lit-tag v="prn"/>
                  <lit-tag v="subj"/>
                  <clip pos="1" side="tl" part="a_pers" />
                  <var n="genre"/>
                  <clip pos="1" side="tl" part="a_nbr" />
                </lu>
                <b/>
                <lu>
                  <lit v="will"/>
                  <lit-tag v="vbmod"/>
                  <lit-tag v="pres"/> <!-- pres? or inf? -->
                </lu>
                <b/>
                <lu>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_verb" link-to="2"/>
                  <lit-tag v="inf"/>
                </lu>
              </chunk>
            </out>
          </when>

          <!-- INF -->
          <when>
            <test>
              <equal>
                <clip pos="1" side="tl" part="a_temps"/>
                <lit-tag v="inf"/>
              </equal>
            </test>
            <out>
              <chunk name="lemma_verb_inf" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="sl" part="a_temps"/></tag>
                </tags>
                <lu>
                  <get-case-from pos="1">
                    <lit v="to"/>
                  </get-case-from>
                  <lit-tag v="pr"/>
                </lu>
                <b/>
                <lu>
                  <clip pos="1" side="tl" part="whole"/>
                </lu>
              </chunk>
            </out>
          </when>

          <when>
            <test>
              <equal>
                <clip pos="1" side="tl" part="a_temps"/>
                <lit-tag v="imp"/>
              </equal>
            </test>
            <out>
              <chunk name="lemma_verb_imp" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="sl" part="a_temps"/></tag>
                </tags>
                <lu>
                  <clip pos="1" side="tl" part="whole"/>
                </lu>
              </chunk>
            </out>
          </when>
        </choose>
      </action>
    </rule>

    <rule comment="RULE: AUX INF">
      <pattern>
        <pattern-item n="aux"/>
        <pattern-item n="verbcj"/>
      </pattern>
      <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <choose>
          <when>
            <test>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="kozala"/>
              </equal>
            </test>
            <call-macro n="conj_verb1">
              <with-param pos="1"/>
            </call-macro>
            <out>
              <chunk name="kozala_pp" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="tl" part="a_verb"/></tag>
                  <tag><clip pos="1" side="tl" part="a_temps"/></tag>
                  <tag><clip pos="1" side="tl" part="a_pers"/></tag>
                  <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                </tags>
                <lu>
                  <get-case-from pos="1">
                    <lit v="prpers"/>
                  </get-case-from>
                  <lit-tag v="prn"/>
                  <lit-tag v="subj"/>
                  <clip pos="1" side="tl" part="a_pers" />
                  <var n="genre"/>
                  <clip pos="1" side="tl" part="a_nbr" />
                </lu>
                <b/>
                <lu>
          	      <var n="verbfinal"/>
          	      <clip pos="1" side="tl" part="lemq"/>
          	    </lu>
                <b/>
                <lu>
                  <clip pos="2" side="tl" part="lem"/>
                  <clip pos="2" side="tl" part="a_verb"/>
          	      <lit-tag v="pprs"/>
                </lu>
              </chunk>
            </out>
          </when>
          <otherwise>
            <choose>
              <when>
                <test>
                  <equal caseless="yes">
                    <clip pos="1" side="sl" part="lem"/>
                    <lit v="kosɛngɛle"/>
                  </equal>
                </test>
                <call-macro n="conj_verb1">
                  <with-param pos="2"/>
                </call-macro>
                <out>
                  <chunk name="kosengele_inf" case="caseFirstWord">
                    <tags>
                      <tag><lit-tag v="SV"/></tag>
                      <tag><clip pos="1" side="tl" part="a_verb"/></tag>
                      <tag><clip pos="1" side="tl" part="a_temps"/></tag>
                      <tag><clip pos="1" side="tl" part="a_pers"/></tag>
                      <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                    </tags>
                    <lu>
                      <get-case-from pos="1">
                        <lit v="prpers"/>
                      </get-case-from>
                      <lit-tag v="prn"/>
                      <lit-tag v="subj"/>
                      <clip pos="2" side="tl" part="a_pers" />
                      <var n="genre"/>
                      <clip pos="2" side="tl" part="a_nbr" />
                    </lu>
                    <b/>
                    <lu>
              	      <lit v="must"/>
                      <lit-tag v="vbmod"/>
              	      <lit-tag v="pres"/>
              	    </lu>
                    <b/>
                    <lu>
                      <clip pos="2" side="tl" part="lem"/>
                      <clip pos="2" side="tl" part="a_verb"/>
              	      <lit-tag v="inf"/>
                    </lu>
                  </chunk>
                </out>
              </when>
              <otherwise> <!-- kokoka(can) -->
                <call-macro n="conj_verb1">
                  <with-param pos="1"/>
                </call-macro>
                <out>
                  <chunk name="kokoka_inf" case="caseFirstWord">
                    <tags>
                      <tag><lit-tag v="SV"/></tag>
                      <tag><clip pos="1" side="tl" part="a_verb"/></tag>
                      <tag><clip pos="1" side="tl" part="a_temps"/></tag>
                      <tag><clip pos="1" side="tl" part="a_pers"/></tag>
                      <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                    </tags>
                    <lu>
                      <get-case-from pos="1">
                        <lit v="prpers"/>
                      </get-case-from>
                      <lit-tag v="prn"/>
                      <lit-tag v="subj"/>
                      <clip pos="1" side="tl" part="a_pers" />
                      <var n="genre"/>
                      <clip pos="1" side="tl" part="a_nbr" />
                    </lu>
                    <b/>
                    <lu>
                      <lit v="can"/>
                      <lit-tag v="vbmod"/>
              	      <lit-tag v="pres"/>
                    </lu>
                    <b/>
                    <lu>
                      <clip pos="2" side="tl" part="lem"/>
                      <clip pos="2" side="tl" part="a_verb"/>
                      <lit-tag v="inf"/>
                    </lu>
                  </chunk>
                </out>
              </otherwise>
            </choose>
          </otherwise>
        </choose>

      </action>
    </rule>

    <rule comment="RULE: NOM VERB">
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="verbcj"/>
      </pattern>
      <action>
        <call-macro n="nom_article1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="conj_verb1">
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <choose>
          <when>
            <test>
              <not>
                <equal>
                  <var n="article"/>
                  <lit v=""/>
                </equal>
              </not>
            </test>
            <out>
              <chunk name="det_nom_verb" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                </tags>
                <var n="article"/>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_nom"/>
                  <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </lu>
                <b/>
                <lu>
                  <var n="verbfinal"/>
          	      <clip pos="2" side="tl" part="lemq"/>
                </lu>
              </chunk>
            </out>
          </when>
          <otherwise>
            <out>
              <chunk name="nom_verb" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SV"/></tag>
                  <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
                </tags>
                <lu>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_nom"/>
                  <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </lu>
                <b/>
                <lu>
                  <var n="verbfinal"/>
          	      <clip pos="2" side="tl" part="lemq"/>
                </lu>
              </chunk>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <!-- <rule comment="RULE: VERB VERB"> -->

    <rule comment="RULE: SENT">
      <pattern>
        <pattern-item n="sent"/>
      </pattern>
      <action>
        <out>
          <chunk name="sent" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
  </section-rules>
