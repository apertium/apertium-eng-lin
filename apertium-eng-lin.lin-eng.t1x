    <def-var n="caseFirstWord"/>
    <def-var n="verbfinal"/>
    <def-var n="article"/>
  </section-def-vars>

  <section-def-lists>
    <def-list n="en_majuscules">
      <list-item v="Mokɔlɔ ya libosó"/>
