  <section-def-macros>

    <def-macro n="conj_verb1" npar="1">
      <choose>
        <!-- Pronouns -->
        <when>
          <test>
            <and>
              <equal>
                <clip pos="1" side="sl" part="a_pers"/>
                <lit-tag v="p3"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="a_nbr"/>
                <lit-tag v="sg"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="a_anim"/>
                <lit-tag v="aa"/>
              </equal>
            </and>
          </test>
          <let>
            <var n="genre"/>
            <lit-tag v="m"/>
          </let>
        </when>
        <when>
          <test>
            <equal>
              <clip pos="1" side="sl" part="a_pers"/>
              <lit-tag v="p2"/>
            </equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="a_nbr"/>
            <lit-tag v="sp"/>
          </let>
          <let>
            <var n="genre"/>
            <lit-tag v="mf"/>
          </let>
        </when>
        <when>
          <test>
            <and>
              <equal>
                <clip pos="1" side="sl" part="a_pers"/>
                <lit-tag v="p3"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="a_anim"/>
                <lit-tag v="nn"/>
              </equal>
            </and>
          </test>
          <let>
            <var n="genre"/>
            <lit-tag v="nt"/>
          </let>
          <let>
            <clip pos="1" side="tl" part="a_nbr"/>
            <lit-tag v="sg"/>
          </let>
        </when>
        <otherwise>
          <let>
            <var n="genre"/>
            <lit-tag v="mf"/>
          </let>
        </otherwise>
      </choose>

      <!-- Default output -->
      <let>
        <var n="verbfinal"/>
        <concat>
          <clip pos="1" side="tl" part="lem"/>
          <clip pos="1" side="tl" part="a_verb"/>
          <var n="temps_chunk"/>
        </concat>
      </let>

      <choose><!-- Present -->
        <when>
          <test>
            <equal>
              <clip pos="1" side="sl" part="a_temps"/>
              <lit-tag v="pres"/>
            </equal>
          </test>
          <modify-case>
            <clip pos="1" side="tl" part="lem"/>
            <lit v="aa"/>
          </modify-case>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="pres"/>
          </let>
          <choose>
            <when> <!-- be, p1, sg -->
              <test>
                <and>
                  <equal>
                    <clip pos="1" side="tl" part="lem"/>
                    <lit v="be"/>
                  </equal>
                  <equal>
                    <clip pos="1" side="sl" part="a_pers"/>
                    <lit-tag v="p1"/>
                  </equal>
                  <equal>
                    <clip pos="1" side="sl" part="a_nbr"/>
                    <lit-tag v="sg"/>
                  </equal>
                </and>
              </test>
              <let>
                <var n="verbfinal"/>
                <concat>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <var n="temps_chunk"/>
                  <clip pos="1" side="tl" part="a_pers"/>
                  <clip pos="1" side="tl" part="a_nbr"/>
                </concat>
              </let>
            </when>
            <when> <!-- p3,sg or sp -->
              <test>
                <and>
                  <equal>
                    <clip pos="1" side="sl" part="a_pers"/>
                    <lit-tag v="p3"/>
                  </equal>
                  <or>
                    <equal>
                      <clip pos="1" side="sl" part="a_nbr"/>
                      <lit-tag v="sg"/>
                    </equal>
                    <equal>
                      <clip pos="1" side="sl" part="a_nbr"/>
                      <lit-tag v="sp"/>
                    </equal>
                  </or>
                </and>
              </test>
              <let>
                <var n="verbfinal"/>
                <concat>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <var n="temps_chunk"/>
                  <clip pos="1" side="tl" part="a_pers"/>
                  <clip pos="1" side="tl" part="a_nbr"/>
                </concat>
              </let>
            </when>
            <!-- <otherwise>
              <let>
                <var n="verbfinal"/>
                <concat>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <var n="temps_chunk"/>
                </concat>
              </let>
            </otherwise> -->
          </choose>
        </when>

        <when> <!-- Past -->
          <test>
            <or>
              <equal>
                <clip pos="1" side="sl" part="a_temps"/>
                <lit-tag v="past"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="a_temps"/>
                <lit-tag v="urp"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="a_temps"/>
                <lit-tag v="dpres"/>
              </equal>
            </or>
          </test>
          <modify-case>
            <clip pos="1" side="tl" part="lem"/>
            <lit v="aa"/>
          </modify-case>
          <let>
            <var n="temps_chunk"/>
            <lit-tag v="past"/>
          </let>
          <choose>
            <when> <!-- be, p1 or p3, sg or sp -->
              <test>
                <and>
                  <equal caseless="yes">
                    <clip pos="1" side="tl" part="lem"/>
                    <lit v="be"/>
                  </equal>
                  <or>
                    <equal>
                      <clip pos="1" side="sl" part="a_pers"/>
                      <lit-tag v="p1"/>
                    </equal>
                    <equal>
                      <clip pos="1" side="sl" part="a_pers"/>
                      <lit-tag v="p3"/>
                    </equal>
                  </or>
                  <or>
                    <equal>
                      <clip pos="1" side="sl" part="a_nbr"/>
                      <lit-tag v="sg"/>
                    </equal>
                    <equal>
                      <clip pos="1" side="sl" part="a_nbr"/>
                      <lit-tag v="sp"/>
                    </equal>
                  </or>
                </and>
              </test>
              <let>
                <var n="verbfinal"/>
                <concat>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <var n="temps_chunk"/>
                  <clip pos="1" side="tl" part="a_pers"/>
                  <clip pos="1" side="tl" part="a_nbr"/>
                </concat>
              </let>
            </when>
            <!-- <otherwise>
              <let>
                <var n="verbfinal"/>
                <concat>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <var n="temps_chunk"/>
                </concat>
              </let>
            </otherwise> -->
          </choose>
        </when>

        <!-- Imperative -->
        <!-- <when>
          <test>
          </test>
        </when> -->
      </choose>
    </def-macro>

    <def-macro n="firstWord" npar="1">
      <choose>
        <when>
          <test>
              <equal>
                <clip pos="1" side="sl" part="_acr"/>
                <lit v=""/>
              </equal>
          </test>
          <choose>
            <when>
              <test>
                <equal>
                  <var n="EOS"/>
                  <lit v="true"/>
                </equal>
              </test>
              <modify-case>
                <clip pos="1" side="tl" part="lem"/>
                <lit v="aa"/>
              </modify-case>
              <let>
                <var n="caseFirstWord"/>
                <lit v="Aa"/>
              </let>
            </when>
            <otherwise>
              <let>
                <var n="caseFirstWord"/>
                <lit v="aa"/>
              </let>
            </otherwise>
          </choose>
        </when>
        <otherwise>
          <let>
            <var n="caseFirstWord"/>
            <lit v="aa"/>
          </let>
        </otherwise>
      </choose>
      <let>
        <var n="EOS"/>
        <lit v="false"/>
      </let>
    </def-macro>

    <def-macro n="nom_article1" npar="1">
      <let>
        <var n="article"/>
        <lit v=""/>
      </let>
      <choose>
         <when><!-- all nouns are assummed to be definite -->
           <test>
             <not>
               <or>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.org"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.top"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.ant"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.al"/>
                 </equal>
               </or>
               </not>
           </test>
           <modify-case>
             <clip pos="1" side="tl" part="lem"/>
             <lit v="aa"/>
           </modify-case>
           <let>
             <var n="article"/>
             <concat>
                 <lit v="^"/>
                 <get-case-from pos="1">
                   <lit v="the"/>
                 </get-case-from>
                 <lit-tag v="det.def"/>
                 <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                 <lit v="$ "/>
             </concat>
           </let>
         </when>

         <when><!-- proper noun with definitive article -->
           <test>
             <and>
               <or>
                 <in caseless="yes">
                   <clip pos="1" side="tl" part="lem"/>
                   <list n="definitenames"/>
                 </in>
                 <equal>
                   <clip pos="1" side="sl" part="a_art"/>
                   <lit-tag v="def"/>
                 </equal>
               </or>
               <or>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.org"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.top"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.ant"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.al"/>
                 </equal>
               </or>
             </and>
           </test>
           <choose>
             <when>
               <test>
                 <equal>
                   <var n="EOS"/>
                   <lit v="true"/>
                 </equal>
               </test>
               <let>
                 <var n="article"/>
                 <concat>
                     <lit v="^"/>
                     <get-case-from pos="1">
                       <lit v="the"/>
                     </get-case-from>
                     <lit-tag v="det.def"/>
                     <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                     <lit v="$ "/>
                 </concat>
               </let>
             </when>
             <otherwise>
               <let>
                 <var n="article"/>
                 <concat>
                     <lit v="^"/>
                       <lit v="the"/>
                     <!--<get-case-from pos="1">
                     </get-case-from>-->
                     <lit-tag v="det.def"/>
                     <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                     <lit v="$ "/>
                 </concat>
               </let>
             </otherwise>
           </choose>
         </when>
       </choose>
     </def-macro>

  </section-def-macros>

  <section-rules>

    <!--********************************* REGLES DE SN ***************************************************** -->
    <rule comment="REGLA: NOM">
     <pattern>
       <pattern-item n="nom"/>
     </pattern>
     <action>
       <call-macro n="nom_article1">
         <with-param pos="1"/>
       </call-macro>
       <call-macro n="firstWord">
         <with-param pos="1"/>
       </call-macro>
       <choose>
         <when>
           <test>
             <not>
               <equal>
                 <var n="article"/>
                 <lit v=""/>
               </equal>
             </not>
           </test>
           <out>
             <chunk name="det_nom" case="caseFirstWord">
               <tags>
                 <tag><lit-tag v="SN"/></tag>
                 <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
               </tags>
               <var n="article"/>
               <lu>
                 <clip pos="1" side="tl" part="lemh"/>
                 <clip pos="1" side="tl" part="a_nom"/>
                 <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                 <clip pos="1" side="tl" part="lemq"/>
               </lu>
             </chunk>
           </out>
         </when>
         <otherwise>
           <out>
             <chunk name="nom" case="caseFirstWord">
               <tags>
                 <tag><lit-tag v="SN"/></tag>
                 <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
               </tags>
               <lu>
                 <clip pos="1" side="tl" part="lemh"/>
                 <clip pos="1" side="tl" part="a_nom"/>
                 <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
                 <clip pos="1" side="tl" part="lemq"/>
               </lu>
             </chunk>
           </out>
         </otherwise>
       </choose>
     </action>
    </rule>

    <rule comment="RULE: NOM YA ADJ"> <!-- ndako ya pembe -> the white house -->
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="yaconn"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="nom_article1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="nom_ya_adj" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <var n="article"/>
            <lu>
              <clip pos="3" side="tl" part="lem"/>
              <clip pos="3" side="tl" part="a_adj"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NOM NA ADJ"> <!-- old Lingala but still in used in formal literature -->
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="na"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="nom_na_adj" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <lu>
              <get-case-from pos="1">
                <lit v="the"/>
              </get-case-from>
              <lit-tag v="det"/>
              <lit-tag v="def"/>
              <lit-tag v="sp"/>
            </lu>
            <b/>
            <lu>
              <clip pos="3" side="tl" part="lem"/>
              <clip pos="3" side="tl" part="a_adj"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NOM ADJ">
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="nom_adj" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <lu>
              <get-case-from pos="1">
                <lit v="the"/>
              </get-case-from>
              <lit-tag v="det"/>
              <lit-tag v="def"/>
              <lit-tag v="sp"/>
            </lu>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="lem"/>
              <clip pos="2" side="tl" part="a_adj"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NOM NUM"> <!-- ndako mibale -> two houses (this can go into t2x)-->
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="num"/>
      </pattern>
      <action>
        <call-macro n="nom_article1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="nom_num" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <!-- <var n="article"/> -->
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NA MBULA NUM"> <!-- na mbula mibale -> two years old -->
      <pattern>
        <pattern-item n="na"/>
        <pattern-item n="mbula"/>
        <pattern-item n="num"/>
      </pattern>
      <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="na_mbula_num" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <lu>
              <clip pos="3" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <lit v="old"/>
              <lit-tag v="adj.sint"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NOM DET"> <!-- mama na-ye, her mom -->
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="det"/>
      </pattern>
      <action>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <modify-case>
          <clip pos="1" side="tl" part="lem"/>
          <lit v="aa"/>
        </modify-case>
        <out>
          <chunk name="nom_det" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <lu>
              <!-- <get-case-from pos="1">
                <clip pos="2" side="tl" part="lem"/>
              </get-case-from> -->
              <clip pos="2" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>

          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="RULE: NOM ADJ YA ADJ"> <!-- nzete monene ya kala, the big old tree -->
      <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="adj"/>
        <pattern-item n="yaconn"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <call-macro n="nom_article1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="firstWord">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="det_nom_adj_ya_adj" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
            </tags>
            <var n="article"/>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="lem"/>
              <clip pos="2" side="tl" part="a_adj"/>
            </lu>
            <b/>
            <lu>
              <clip pos="4" side="tl" part="lem"/>
              <clip pos="4" side="tl" part="a_adj"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

